<?xml version="1.0" encoding="utf-8"?>
<!--    外观检验页面子项布局    -->
<!--  android:background="@color/bootstrap_brand_info"  -->
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:layout_marginTop="2dp"
    android:layout_marginBottom="3dp"
    android:layout_marginLeft="5dp"
    android:layout_marginRight="5dp"
    android:background="@drawable/blue_button_background"
    tools:context="com.org.huanjianmvp.Activity.ListDatas">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/inspect_Text"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:layout_marginLeft="5dp"
            android:gravity="center_vertical"
            android:textColor="@color/bootstrap_gray_dark"
            android:text="外观检验"
            />
        <!--<Spinner
            android:id="@+id/inspectSpinner"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:overlapAnchor="false"
            android:popupBackground="#818a91"
            android:entries="@array/spinnerValue"
            >
        </Spinner>-->

        <TextView
            android:id="@+id/textResult"
            android:gravity="center"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:textColor="@color/colorRed"
            android:layout_weight="1"/>
    </LinearLayout>



</LinearLayout>
